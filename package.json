{
  "name": "happy-lunch",
  "version": "1.0.0",
  "description": "妈妈再也不用担心我吃不上午饭了",
  "private": true,
  "egg": {
    "typescript": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --sticky --title=egg-server-happy-lunch",
    "stop": "egg-scripts stop --title=egg-server-happy-lunch",
    "dev": "egg-bin dev -r egg-ts-helper/register --sticky",
    "debug": "egg-bin debug -r egg-ts-helper/register --sticky",
    "test-local": "egg-bin test -r egg-ts-helper/register",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov -r egg-ts-helper/register",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean"
  },
  "dependencies": {
    "@types/mz": "0.0.32",
    "egg": "^2.14.1",
    "egg-redis": "^2.1.0",
    "egg-scripts": "^2.6.0",
    "egg-socket.io": "^4.1.5",
    "egg-view-nunjucks": "^2.2.0",
    "mz": "^2.7.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.6.2",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "egg-ts-helper": "^1.16.1",
    "tslib": "^1.9.0",
    "tslint": "^4.0.0",
    "typescript": "^3.2.1"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "xiaodabao",
  "license": "MIT"
}
